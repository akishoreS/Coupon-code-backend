{
  "version": 2,
  "builds": [
    {
      "src": "CouponCreationBackend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "CouponCreationFrontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "CouponCreationBackend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "CouponCreationFrontend/build/index.html"
    }
  ]
}
